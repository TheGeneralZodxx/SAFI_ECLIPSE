	<queryString>
		<![CDATA[select distinct(x.id),* from ( 
SELECT
   m.id,
   m.fecha_alta,
	m.numero_asiento AS asiento,
	s.codigo::numeric as codigo_servicio,
	ejer.anio as ejercicio,
	(cb.numero || ' - ' || cb.descripcion) AS cuenta,
	 to_char( m.fecha_alta, 'dd/MM/yyyy') AS fecha ,
	(s.codigo || ' - ' || s.abreviatura) AS servicio,
	m.numero_orden AS numero_orden,
	f.tipoordengasto_id AS tipo_ordenGasto,
	f.tipofondo_id AS tipo_fondo,
	f.tipoimputacion_id AS tipo_imputacion,
	m.importe AS importe,
	m.descripcion AS descripcion,
	(exp.codigo || '-' || exp.numero || '/'|| exp.anio) AS expediente,
	m.numero_comprobante AS numero_comprobante     
FROM public.movimientos m 
	INNER JOIN cuentas_bancarias cb ON m.cuentabancaria_id=cb.id
	INNER JOIN servicios s ON cb.servicio_id = s.id
	INNER JOIN movimientos_funciones AS mf ON mf.movimiento_id=m.id
	INNER JOIN funciones AS f ON f.id=mf.funciones_id
	INNER JOIN expedientes as exp on m.expediente_id= exp.id
	INNER JOIN ejercicios as ejer on m.ejercicio_id=ejer.id
WHERE m.estado =true 	
	$P!{query}
	) as x order by x.ejercicio DESC,x.codigo_servicio ASC,x.asiento DESC,x.fecha_alta desc]]>
	</queryString>
