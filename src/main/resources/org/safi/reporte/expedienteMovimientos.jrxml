	<queryString>
		<![CDATA[SELECT 
	e.codigo||'-'||e.numero||'/'||e.anio AS expediente,
	s.codigo||' - '||s.descripcion AS servicio,
	ej.anio,
	cb.numero||' - '||cb.descripcion AS cuenta,
	m.numero_asiento AS asiento,
	m.numero_orden as numero_orden,
	m.numero_comprobante as numero_comprobante,
	m.numero_pedido_fondo as numero_pedido_fondo,
	m.fecha_alta,
	f.tipoordengasto_id||'-'||f.tipofondo_id||'-'||f.tipoimputacion_id AS clave,
	f.tipoordengasto_id AS orden,
	f.tipofondo_id AS fondo,
	f.tipoimputacion_id AS imputacion,
	m.descripcion,
	m.importe	
FROM expedientes AS e
	INNER JOIN movimientos AS m ON m.expediente_id=e.id
	INNER JOIN cuentas_bancarias cb ON m.cuentabancaria_id= cb.id
	INNER JOIN servicios s ON s.id = cb.servicio_id
	INNER JOIN ejercicios ej ON ej.id = m.ejercicio_id
	INNER JOIN movimientos_funciones AS mf ON mf.movimiento_id=m.id
	INNER JOIN funciones AS f ON mf.funciones_id=f.id
WHERE m.estado=true
	AND f.tipoacumulador_id IN (1,7)
	AND e.codigo= $P{codigo} 
	AND e.numero= $P{correlativo} 
	AND e.anio= $P{anio} 
	AND ej.id=$P{idEjercicio} 
	AND cb.numero=$P{numeroCuenta}
ORDER BY ej.anio,cb.numero,m.numero_asiento]]>
	</queryString>
