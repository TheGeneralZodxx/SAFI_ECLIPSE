	<parameter name="historial" class="java.lang.Long"/>
	<queryString>
		<![CDATA[SELECT 
	m.nombre AS municipio,
	hm.numero_inscripcion_habilitacion AS numero,
	hm.fecha_vencimiento AS fec
FROM proveedores AS p
	INNER JOIN historiales_proveedor AS h ON h.proveedor_id = p.id
	LEFT JOIN proveedores_habilitaciones_municipales AS phm ON phm.proveedor_id = p.id
	LEFT JOIN habilitaciones_municipales AS hm ON hm.id=phm.habilitacionesmunicipales_id
	LEFT JOIN municipios AS m ON m.id= hm.municipio_id 
WHERE
	h.id = $P{historial}
	AND hm.estado = TRUE
ORDER BY municipio]]>
	</queryString>
	<field name="municipio" class="java.lang.String"/>
	<field name="numero" class="java.lang.String"/>
	<field name="fec" class="java.sql.Timestamp"/>
