	<queryString>
		<![CDATA[SELECT
	auditoria.id,
	auditoria.fecha,	
	usuarios.apellido_nombre,
	usuarios.nombre_usuario AS uss, 
  	(SELECT servicios.codigo|| ' - '  ||servicios.abreviatura AS servicio FROM servicios 
		WHERE auditoria.servicio_id = servicios.id ) AS servicio, 
 	auditoria.accion,
	auditoria.valoranterior,
 	auditoria.valoractual, 
  	  ejercicios.anio 
FROM auditoria
	INNER JOIN usuarios  ON auditoria.usuario_id = usuarios.id
	INNER JOIN ejercicios  ON auditoria.ejercicio_id = ejercicios.id
 WHERE 1=1  $P!{query}]]>
	</queryString>
